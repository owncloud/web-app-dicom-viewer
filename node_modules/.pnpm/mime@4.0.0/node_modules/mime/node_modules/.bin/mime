#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/k2/JankariTech/web-app-dicom-viewer/node_modules/.pnpm/mime@4.0.0/node_modules/mime/bin/node_modules:/Users/k2/JankariTech/web-app-dicom-viewer/node_modules/.pnpm/mime@4.0.0/node_modules/mime/node_modules:/Users/k2/JankariTech/web-app-dicom-viewer/node_modules/.pnpm/mime@4.0.0/node_modules:/Users/k2/JankariTech/web-app-dicom-viewer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/k2/JankariTech/web-app-dicom-viewer/node_modules/.pnpm/mime@4.0.0/node_modules/mime/bin/node_modules:/Users/k2/JankariTech/web-app-dicom-viewer/node_modules/.pnpm/mime@4.0.0/node_modules/mime/node_modules:/Users/k2/JankariTech/web-app-dicom-viewer/node_modules/.pnpm/mime@4.0.0/node_modules:/Users/k2/JankariTech/web-app-dicom-viewer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
