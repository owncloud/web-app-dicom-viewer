{
  "name": "web-app-dicom-viewer",
  "version": "0.0.0",
  "description": "ownCloud Web DICOM Viewer",
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "lint": "eslint './*.{ts,js}' '{src,tests}/**/*.{js,ts,vue}' --color",
