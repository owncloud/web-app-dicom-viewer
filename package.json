    "pinia": "2.1.4",
    "vuex": "4.1.0"
  },
  "pnpm": {
    "overrides": {
      "mime": "^3.0.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "design-system*"
      ]
    }
  },
  "packageManager": "pnpm@8.14.1"
