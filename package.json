  },
  "devDependencies": {
    "@cucumber/cucumber": "10.0.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@ownclouders/eslint-config": "0.0.1",
    "@ownclouders/extension-sdk": "0.0.5-alpha.2",
    "@ownclouders/prettier-config": "0.0.1",
    "@ownclouders/tsconfig": "0.0.5-alpha.1",
    "@playwright/test": "^1.41.1",
    "@types/jest": "29.5.1",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@vitejs/plugin-vue": "4.5.2",
    "@vitest/coverage-v8": "1.1.0",
    "axios": "^1.6.7",
    "happy-dom": "12.10.3",
    "join-path": "^1.1.1",
    "@vue/test-utils": "2.4.4",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "23.0.1",
    "lodash": "^4.17.21",
    "prettier": "2.6.2",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.1.0",
    "vitest-mock-extended": "1.3.1",
    "xml-js": "^1.6.11",
    "vue": "3.3.8",
    "vue3-gettext": "2.5.0-alpha.1"
  },
  "dependencies": {
    "@cornerstonejs/core": "^1.13.0",
    "@cornerstonejs/dicom-image-loader": "^1.13.0",
    "dicom-parser": "^1.8.21"
  },
  "peerDependencies": {
    "@casl/ability": "^6.3.3",
    "@casl/vue": "^2.2.1",
    "@ownclouders/web-client": "0.0.1",
    "@ownclouders/web-pkg": "0.0.5-alpha.9",
    "lodash-es": "^4.17.21",
    "luxon": "^2.4.0",
    "pinia": "2.1.4",
    "vuex": "4.1.0"
  },
  "pnpm": {
    "overrides": {
      "mime": "^3.0.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "design-system*"
      ]
    }
  },
  "packageManager": "pnpm@8.14.1"
}
