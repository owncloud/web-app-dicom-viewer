    "luxon": "^2.4.0",
    "pinia": "2.1.7",
    "vuex": "4.1.0"
  },
  "pnpm": {
    "overrides": {
      "mime": "^3.0.0",
      "glob": "^10.5.0",
      "test-exclude": "^7.0.1"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "design-system*"
      ]
    }
  },
  "packageManager": "pnpm@9.4.0"
