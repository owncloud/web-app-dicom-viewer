    "@ownclouders/tsconfig": "0.0.5-alpha.1",
    "@types/jest": "29.5.1",
    "@types/node": "^20.2.3",
    "@vitejs/plugin-vue": "4.5.2",
    "@vitest/coverage-v8": "1.1.0",
    "@vue/test-utils": "v2.4.0-alpha.0",
    "eslint": "8.31.0",
    "happy-dom": "12.10.3", 
    "jsdom": "23.0.1",
    "prettier": "2.6.2",
    "typescript": "5.3.3",
    "vite": "^5.0.0", 
    "vitest": "^1.1.0",
    "vitest-mock-extended": "1.3.1",
    "vue": "3.3.4", 
    "vue3-gettext": "2.5.0-alpha.1"
