    "@ownclouders/prettier-config": "0.0.1",
    "@ownclouders/tsconfig": "0.0.6",
    "@playwright/test": "^1.45.1",
    "@types/jest": "29.5.12",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@vitejs/plugin-vue": "5.0.5",
    "@vitest/coverage-v8": "1.6.0",
    "@vue/test-utils": "2.4.6",
    "axios": "^0.30.0",
    "eslint": "8.56.0",
